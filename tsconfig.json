{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "obsidian": [
        "node_modules/obsidian"
      ]
    },
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*.ts"
  ]
}